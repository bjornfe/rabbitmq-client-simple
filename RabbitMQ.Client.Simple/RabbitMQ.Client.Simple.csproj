<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Bjørn F. Esaiassen</Authors>
    <Company>iCsys AS</Company>
    <Description>A simplified client for working with RabbitMQ.Client. Supports both "fire and forget" and RPC calls.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/bjornfe/rabbitmq-client-simple.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bjornfe/rabbitmq-client-simple.git</RepositoryUrl>
    <PackageTags>RabbitMQ</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MessagePack" Version="1.8.74" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="RabbitMQ.Client" Version="5.1.2" />
  </ItemGroup>

</Project>
